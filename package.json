{
  "name": "nadia-jupiter-one-test",
  "version": "1.0.0",
  "description": "Testing JupiterOne Test Plan",
  "main": "index.js",
  "scripts": {
    "test:local": "cypress run --env fileConfig=local --spec '**/*.feature'",
    "test:prod": "cypress run --env fileConfig=prod --spec '**/*.feature'",
    "test:qa": "cypress run --env fileConfig=qa --spec '**/*.feature'",
    "test:staging": "cypress run --env fileConfig=staging --spec '**/*.feature'",
    "prettier:format": "prettier --config .prettierrc 'cypress/**/*.js' --write"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hamdynad/nadia-jupiter-one-test.git"
  },
  "author": "nadia hamdy",
