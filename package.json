  "bugs": {
    "url": "https://github.com/hamdynad/nadia-jupiter-one-test/issues"
  },
  "homepage": "https://github.com/hamdynad/nadia-jupiter-one-test#readme",
  "devDependencies": {
    "axe-core": "^4.4.1",
    "cypress": "^9.5.0",
    "cypress-axe": "^0.14.0",
    "cypress-cucumber-preprocessor": "4.3.1",
    "prettier": "2.5.1"
  }
}
